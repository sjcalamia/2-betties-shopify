      font-size: calc(100vw / 62);
    }

    .slide .slide__price-point {
      width: calc(100vw / 11);
      height: calc(100vw / 11);
    }

    .slide .slide__price-point .slide__price {
      padding-top: calc(100vw / 40);
      font-size: calc(100vw / 50);
      line-height: calc(100vw / 47);
    }

