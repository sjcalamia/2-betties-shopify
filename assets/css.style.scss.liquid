  opacity: 0;
}

.wf-active body,
.wf-inactive body {
  -webkit-transition: opacity 1s ease-out;
          transition: opacity 1s ease-out;
}

/* Media styles */
@media (min-width: 769px) {
  .template-index .main-content,
  .template-index .footer {
    opacity: 1;
  }

  .main-content {
    opacity: 1;
    margin: 0px;
    top: 0px;
  }
