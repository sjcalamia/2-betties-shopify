/* Nutrition Page
/* ------------------------------------------------------------------------- */
.nutrition {
  text-align: left;

  h3, h4 {
    font-size: 2em;
    margin: 0em auto 1.5em;
    padding-top: 0;
  }
  section {
    margin: 4em auto;
    &.wrapper.grid {
      @media (max-width: 1100px) {
        margin-left: 3vw;
        margin-right: 3vw;
      }
    }
    li {
      list-style: inside;
      list-style-type: disc;
      padding: 1em 0em 0em 1em;
    }
  }
  .hero {
    &.heading--page {
      background-image: url({{ '2Betties-nutrition-mission.jpg' | asset_img_url: 'master' }});
      background-position: right;
    }

    &.protein, &.sugars {
      background-position: top center;
    }
    &.sugars {
      background-image: url({{ 'kidos.jpg' | asset_url }});
    }
    &.protein {
      background-image: url({{ 'running.jpg' | asset_url }});
    }
  }

  .intro {
    padding: 1.5em 0 1em;
    text-align: center;
    @extend .grid__item;
    @extend one-whole;

    .hed {
      margin: 0 auto 1em;
    }
    .lede {
      @extend .three-quarters;
      @extend .centered;
      @media (max-width: $small) { left: 0px; }
    }
    + .dek {
      padding: 0;
      margin: 1.5em auto 1em;
      text-align: center;
      @include font("franklin");
      font-size: 4.5vw;
      @extend .grid__item;
      @extend .one-whole;
    }
  }

  .lesson {
    margin: 0 auto 6.5vh;
    @extend .one-whole;
    .qa {
      //padding: 2vh 2vw;
      @extend .five-twelfths;
      @extend .small--one-whole;
      text-align: left;
      .question {
        padding: 1em 0 0;
      }
      .answer {
        position: relative;
        @extend .five-sixths;
        //@extend .push--one-twelfth;
        padding: 1em;
        padding-left: 0;
      }
    }
    p { margin-bottom: 2em; }
    p + ul { margin-top: -2em; }
    figure {
      @extend .seven-twelfths;
      @extend .small--one-whole;
      img {
        height: 100%;
        @include prefix(mask, url({{'border-distressed.svg' | asset_url }}));
        @include prefix(mask-size, 104%);
        @include prefix(mask-position, center center);
      }
    }
  }

  article {
    clear: both;
  }

  figure {
    padding: 0;
    margin: 0;
    figcaption {
      text-align: center;
      margin-bottom: 1.5em;
    }
    &.left img {
      float: left;
    }
    &.right img {
      float: right;
    }
    img {
      padding: 0;
      margin: 0;
    }
    ol {
      list-style-type: decimal;
      list-style-position: inside;
      li {
        list-style-type: decimal;
        text-align: justify;
        padding: 0.75em;
        :first-child {
          display: inline;
        }
      }
    }
  }


}
.nutrition-label {
  figure {
    margin-top: 2em;

    figure-caption {
      text-align: center;
      font-size: .75em;
      margin-bottom: 1em;
    }
    img {
      margin-top: 1.5em;
    }
  }
}

