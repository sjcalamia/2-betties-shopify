    img {
      position: absolute;
      top: 50%;
      left: 50%;
      @include transform(translate3d(-50%, -50%, 0) scale(1.8));
    }
  }
}

/* Media styles */
@media (max-width: 768px) {
  .flexslider-thumb {

    .slide {
      @include transition(none);
    }

    .flex-control-thumbs {
      margin-top: 0px;
      width: 100%;
      position: absolute;
      right: 50%;
      top: 90%;
      bottom: initial;
      z-index: 2;

      @include transform(translate3d(50%, -50%, 0));
    }

    .flex-control-thumbs li{
