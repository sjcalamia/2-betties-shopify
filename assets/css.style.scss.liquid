            background-size: 100% 300%;
            &:before, &:after {
              background-color: $colorLink;
            }
          }
        }
      }

      &.active .nav-list__link {
        @extend %active-link;
      }

      &.active .nav-list__link:before,
      &.active .nav-list__link:after {
        background-color: $colorHighlight;
      }
    }

    & > .has-dropdown {
