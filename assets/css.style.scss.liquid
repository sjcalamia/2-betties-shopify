    }
    .nav-list__item {
      &.nav-list__item--add {
        margin-left: 1.5vw;
        margin-right: 1.5vw;
        &:first-of-type { margin-left: 0; }
        &:last-of-type { margin-right: 0; }
      }
      &:hover, &:focus {
        background-color: $base2;
      }
      @media (max-width: $medium) {
        @include square(48vw);
        padding-bottom: 3vw;
      }
      &.nav-list__item--search {
        width: 100%;
        height: auto;
      }
    }

    .nav-list__item > .nav-list__link {
      padding: 9px 38px 10px 59px;

      &:hover, &:focus {
        color: $colorLink;
      }
    }

    .nav-list__item:last-child > .nav-list__link {
      padding-bottom: 27px;
    }

    .nav-list__dropdown .nav-list__item > .nav-list__link {
      padding-left: 79px;
    }

    & > .nav-list__item--add {
      .nav-list__image,
      .nav-list__desc {
        display: none;
      }

      .nav-list__title {
        font-family: $bodyFontStack;
        font-size: 16px;
        font-weight: normal;
      }
    }
  }

  .has-dropdown.show-dropdown {
    background-color: $base3;
    color: $base00;

    & > .icon--plus-small {
      background-color: $base3;
      color: $base03;

      &:before,
      &:after {
        background-color: $base03;
      }
    }

    & > .nav-list__link {
      color: $colorMenuText;
      @media (max-width: $medium) {
        color: $base1;
      }
    }

    & > .icon--plus-small:after {
      opacity: 0;
    }
  }

  .has-dropdown.show-dropdown > .nav-list__dropdown {
    display: block;
    @media (max-with: $medium) {
      display: flex;
    }
  }

  .has-dropdown > .nav-list__dropdown{
    display: none;
  }

  .nav-list__item--divider {
    margin: 37px 0;
    width: 100%;
    height: 3px;
    background-color: $colorNeutral;

    &:hover, &:focus {
      background-color: $colorNeutral;
    }
  }

  .nav-list__item--logout-desktop {
    display: none;
  }
}

.nav-list .form-wrapper {
  padding: 0 38px;
}


/* Media styles */
@media (min-width: 769px) {
  .drawer--menu {
    position: relative;

    .drawer__btn-close,
    .drawer__btn-open {
      display: none;
    }

    .drawer__header {
      margin: 0 auto;
      padding: 17px 0;
    }

    .store-desc--mobile {
      display: none;
    }
  }

  .drawer--menu .drawer__header>.store-desc {
    display: inline-block;
  }

  .nav-list {
    margin: 0 auto;
    //padding-top: 7px;
    width: 100%;
    position: relative;
    text-align: center;

    .nav-list__item {
      display: inline-block;
      //padding-bottom: 0.5em;
      //
      %active-link {
        color: $base00;
        background-color: $base3;
        @include svg-dot($base3);
        background-size: 100% 250%;
        background-position: 50% 50%;
      }

      &:hover, &:focus {
        //background-color: $colorNeutral;
        //@include radial-gradient($base3,$base1,$base3,75%,85%);
        .nav-list__link {
          @extend %active-link;
          color: $colorLink;
        }
      }

      &.has-dropdown {
        &:hover, &:focus {
            background-size: 100% 300%;
