}

/* Hero images
/* -------------------------------------------------------------------------- */

.hero {
  text-align: center;
  background-size: cover;
  width: 100vw;
  font-size: 16px;
  height: 38.05vw;
  //min-height: 28.125em;
  margin-bottom: 4vw;

  .hed, .title {
    color: $base1;
    @include font($rooster);
    text-transform: uppercase;
    font-size: 3vw;
    font-weight: 300;
    padding-top: 14vw;
  }
  .dek, .subtitle {
    color: $base0;
    font-weight: 700;
    font-family: sofia-pro;
    letter-spacing: -.025em;
    font-size: 5vw;
    margin-top: .5vw;
    line-height: 1em;
  }
}

/* Body Text Block
 * -------------------------------------------------------------------------- */
.grid.block-text {
  margin-top: 7vw;
  font-size: 1.5vw;
  @media (max-width: $medium) {
    font-size: 2em;
  }
  .content {
    @extend .grid-item;
    @extend .large--one-half;
    @extend .push--large--one-quarter;
    @extend .medium--three-quarters;
    @extend .push--medium--one-eighth;
    @extend .medium-down--one-whole;
    @media (min-width: $medium) {
      left: 34vw;
    }
    @media (min-width: $large) {
      left: 27vw;
    }
    p {
      padding-bottom: 2em;
    }
  }
}


/* Links
/* -------------------------------------------------------------------------- */
a {
  color: inherit;
  text-decoration: none;
  @include transition(color 300ms ease-in-out);
}

button {
  padding: 0;
  background-color: transparent;
  border: none;
  @include transition(color 300ms ease-in-out);
}

/* Lists
/* -------------------------------------------------------------------------- */
ul, ol {
  margin: 0;
  padding: 0;
}

ul { list-style: none; }
ol { list-style: decimal; }

/* Tables
/* -------------------------------------------------------------------------- */
table {
  width: 100%;
  margin-bottom: 1em;
  border-collapse: collapse;
  border-spacing: 0;
}

.table-wrap {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

th {
  font-weight: bold;
}

th, td {
  text-align: left;
  padding: $gutter / 2;
  border: 1px solid $colorBorder;
}

/* Helper Classes
/* -------------------------------------------------------------------------- */
.clearfix {
  clear: both;
  &:after {
    content: "";
    display: table;
    clear: both; }
  *zoom: 1;
}

.display-table {
  display: table;
  table-layout: fixed;
  width: 100%;
}

.display-table-cell {
  display: table-cell;
  vertical-align: middle;
  float: none;
}

.elem-hide {
  visibility: hidden;
}

.display-none {
  display: none;
}

.fallback-text {
  display: none;
}

.grid--in-row {
  margin-bottom: 72px;
  display: inline-block;
  vertical-align: top;
  max-width: 260px;

  .btn--full {
    width: calc(100% + 2px);
    left: -1px;
    border: 1px solid #45b1c7;
    border-radius: 0;
  }
  .btn--default {
    font-size: 1.5vw;
    padding: 1vw;

  }
}

.grid--in-row-fix {
  max-width: 260px;
  max-height: 320px;
  width: 100%;
  height: 100%;
}


/* ==========================================================================
