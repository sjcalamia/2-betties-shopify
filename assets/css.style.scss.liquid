/* ==========================================================================
   Accordions
   ========================================================================== */

/* Homepage accordion
/* -------------------------------------------------------------------------- */
.accordion {
  @extend .grid;
  @extend .wrapper;
  padding: 4rem 0 0 0;
  font-size: 1rem;
  @media (max-width: $small) {
    padding-left: 3vw;
  }

  .accordion__header {
    @extend .grid__item;
    @extend .large--five-sixths;
    @extend .push--large--one-twelfth;
    @extend .medium--one-whole;

    text-align: left;
    .hed {
      color: $base1;
      font-size: 2em;
      @include font($frank);
    }
  }

  .accordion__tabgroup {
    position: relative;
    display: block;
    margin-bottom: 4vw;
    @extend .one-whole;

    @extend .grid__item;
    @extend .medium--one-whole;
    @media (max-width: $small) {
      width: 95%;
    }


    .accordion__tabitem {
      &:not(:last-of-type) {
        border-bottom: 1px solid transparentize($base01,.75);
      }
      padding: 2em 0;
      transition: all 420ms ease-out;
      &.closed {
        .accordion__tabcontent {
        }
      }

      .accordion__tabtitle {
        font-size: 2em;
        @include font($sofia);
      }
      .accordion__tabcontent {
        font-size: 1em;
        opacity: 0;
        height: 0px;
        overflow: hidden;
        margin: 0;
        margin-top: 0.001em;
        transition: all $anispd_fast ease-out;
        color: transparentize($base00, 1);
      }
      &.open, &:first-of-type:not(.closed) {
        .accordion__tabcontent {
          opacity: 1;
          color: $base00;
          margin: 0;
          margin-top: 1em;
          height: auto;
        }
      } 
    }
  }

  .accordion__tabtitle {
    &::before, &::after {
      color: $base1;
    }
  }
  
  a.button.more {
    clear: both;
    color: $base0;
    background-color: $base1;
    width: fit-content;
    margin: 1vw auto 0;
    /* margin: 0vw 0vw 0vw 2vw; */
    text-align: center;
    padding: 1em 2em 1em 2em;
    border-radius: .25vw;
    text-transform: uppercase;
    font-weight: 700;
    @include font($sofia);
    display: block;
    transition: all $anispd_fast ease-in-out;
    &:hover, &:focus {
      color: $base1;
      background-color: $base2;
    }
  }
}
.homepage .accordion {

}

/* ==========================================================================
   Sliders
   ========================================================================== */

/* Homepage slider
/* -------------------------------------------------------------------------- */
.flexslider-thumb {
  margin: 0;
  border: none;
  border-radius: 0;
  height: 38.05vw;
  //border-bottom: 3px solid $base1;

  .slide {
    @extend %homepage_slider_height;
    overflow: hidden;
    position: relative;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center right;
    &.slide--3 {
      width: 29vw;
    }
    .slide__heading {
      font-size: 3.2vw;
    }
    img {
      width: 100%;
      display: none;
    }
    .tobe svg {
      height: 18vw;
    }
    h3.slide__heading {
      font-size: 1.5vw;
      .slide__link {
        color: white;
        width: 27.5vw;
      }
    }
  }

  .flex-direction-nav {
    display: none;
  }

  .flex-control-thumbs {
    margin-top: -150px;
    padding-top: 20px;
    width: 70px;
    position: absolute;
    right: 5px;
    top: 50%;
    bottom: initial;
    z-index: 2;
    counter-reset: flex-controls;
    overflow: visible;
    li {
      counter-increment: flex-controls;
      margin: 0 auto;
      margin-bottom: 15px;
      width: 70px;
      height: 70px;
      display: block;
      position: relative;
      overflow: hidden;
      float: none;
      border-radius: 50%;
      background-color: $base2;
      @include svg-dot($base2);
      //background: url("data:image/svg+xml,%3Csvg width='64' height='64' viewBox='0 0 64 64' xmlns='http://www.w3.org/2000/svg'%3E%3Cstyle%3E.dot %7B fill%3A %233DB1C8%3B %7D%0D%0A%3C/style%3E%3Cpath class='dot' d='M11.208 57.09C-2.69 45.495-3.824 25.602 8.708 10.637c11.96-14.273 32.946-14.173 45.773.244 9.583 10.457 14.206 30.376 2.85 41.493-10.92 11.3-32.847 15.17-46.122 4.717'/%3E%3C/svg%3E") no-repeat;
      z-index: 2;
      @include transition(transform 220ms ease-in-out);

      &.active-thumb {
        @include svg-dot($base1);
      }

      &::before {
        content: counter(flex-controls);
        @include font($rooster);
        font-size: 3em;
        //background: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8.14 30.28'><defs><style>.cls-1{fill:#fefefe;}</style></defs><title>Num1</title><g id='Layer_2' data-name='Layer 2'><g id='CarouselIndicators'><path id='Num1' class='cls-1' d='M8.14,6.88Q7.61,14.7,7.08,22.53C7,24,7,25.53,6.79,27c-.33,2.49-1.34,3.5-3,3.24-1.42-.22-2.12-1.68-2-3.93q.52-7.27,1-14.54c0-.68,0-1.37,0-2.06a1.71,1.71,0,0,0-1.52-2C-.08,7.43-.3,6.82.35,5.59a17.24,17.24,0,0,1,4.4-5A1.65,1.65,0,0,1,7.64,1.66,29.47,29.47,0,0,1,8.14,6.88Z'/></g></g></svg>") no-repeat;
        color: $base0;
        height: 50%;
        display: inline-block;
        position: relative;
        top: -.25em;
        left: -.05em;
      }

      img {
        opacity: 0;
        position: relative;
        top: -100%;
      }

      &:hover:not(.active-thumb) {
        @include transform(scale(1.34));
        @include svg-dot(lighten($base1,20%));

      }

      &.active-thumb {
        border-color: transparent;
      }
    }
  }
}

%homepage_slider_height {
  height: 38.05vw;
  @media (max-width: $medium) {
    min-height: 300px;
    .slides {
      // TODO: This is kind of a hack to make the images look good 
      // This should be controlled through admin settings per image, not hard coded
      .slide__add_heading { display: none; }
      .slide {
        background-position: 85% center;
      }
      .slide--1 {
        background-position: 75% center;
      }
      .slide--2 {
        background-position: 80% center;
        @media (max-width: $medium) {
          .slide__link {
            display: none;
          }
        }
      }
    }
  }
  @media (min-width: $large) {
    min-height: 28.125em;
  }
