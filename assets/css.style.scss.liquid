}

@-ms-keyframes openPopup {
  0% {
    transform: scale(0);
    visibility: hidden;
  }

  100% {
    transform: scale(1);
    visibility: visible;
  }
}

@keyframes openPopup {
  0% {
    transform: scale(0);
    visibility: hidden;
  }

  100% {
    transform: scale(1);
    visibility: visible;
  }
}

@media (min-width: 769px) and (max-width: 1024px){
