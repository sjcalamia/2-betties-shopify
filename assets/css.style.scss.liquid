/* CTA buttons
/* -------------------------------------------------------------------------- */
.cta-btn-list {
  padding-top: 52px;

  .btn--cta {
    margin-bottom: 7px;
    padding: 17px 0px;
    min-width: 280px;
    display: inline-block;
